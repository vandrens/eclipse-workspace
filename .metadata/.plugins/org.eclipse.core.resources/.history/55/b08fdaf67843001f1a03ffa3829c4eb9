package curso_udemy_aula1;

public class aula2 {

	public static void main(String[] args) {
		double b, B, h, area;
		b=6.0;
		B=8.0;
		h=5.0;
		area = (b+B)/2*h;
		System.out.printf("%.3f%n", area);
		
		float a, c;
		double resultado;
		a=5;
		c=2;
		resultado = a/c;
		System.out.print(resultado);


	}

}
