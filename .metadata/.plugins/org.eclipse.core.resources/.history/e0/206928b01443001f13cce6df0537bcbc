package curso_udemy_aula1;

import java.util.Locale;

public class Main {

	public static void main(String[] args) {
		double x=2.1415;
		System.out.print("Hello world!");
		System.out.printf("%.3f%n",x);
		System.out.print("Hello world!");
		Locale.setDefault(Locale.US);
		System.out.printf("%.3f%n",x);

		
	}

}
